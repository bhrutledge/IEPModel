    <xs:include schemaLocation="../../../IEP/schema/CommonSolar.xsd"/>
    <xs:include schemaLocation="../../../IEP/schema/ArrayLayout.xsd"/>
    <xs:include schemaLocation="../../../IEP/schema/CommonSystemProperties.xsd"/>
    <xs:include schemaLocation="../../../IEP/schema/Project.xsd"/>
    <xs:include schemaLocation="../../../IEP/schema/PvSystem.xsd"/>
    <xs:include schemaLocation="../../../IEP/schema/RoofFace.xsd"/>
    <xs:include schemaLocation="IEPModel-Draft.xsd"/>
</xs:schema>
